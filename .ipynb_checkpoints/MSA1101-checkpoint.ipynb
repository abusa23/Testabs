    "def recursive_ab(a, b):\n",
    "    if a < b:\n",
    "        return recursive_ab(b, a)\n",
    "    elif b != 0:\n",
    "        return(a + recursive_ab(a, b-1))\n",
    "    else: return 0\n",
    "a = 1  \n",
    "b = 2\n",
    "print(recursive_ab(a, b))"
   ]
